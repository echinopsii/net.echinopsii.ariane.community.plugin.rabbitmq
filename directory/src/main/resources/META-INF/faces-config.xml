              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
    <managed-bean>
        <managed-bean-name>rabbitmqComponentNewController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.plugin.rabbitmq.directory.controller.rabbitmqcomponent.RabbitmqComponentNewController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
