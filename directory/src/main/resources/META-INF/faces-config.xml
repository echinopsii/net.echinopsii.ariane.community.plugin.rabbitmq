    <managed-bean>
        <managed-bean-name>rabbitmqComponentsListController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.plugin.rabbitmq.directory.controller.rabbitmqcomponent.RabbitmqComponentsListController</managed-bean-class>
